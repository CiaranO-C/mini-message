<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <a href="/new"><h3>Send a message!</h3></a>
    <% messages.forEach(msg => { %>
      <p><%= msg.text%></p>
      <p><%= msg.user%></p>
      <p><%= msg.added%></p>
      <% }) %>
  </body>
</html>
